		<xacro:default_link_collision link_name="leg_r_link06" friction="0.0" />
		<xacro:default_link_collision link_name="leg_r_link07" friction="0.01" />
		<xacro:default_link_collision link_name="leg_r_link08" friction="0.0" />
		<xacro:default_link_collision link_name="leg_r_link09" friction="0.0" />
		<xacro:default_link_collision link_name="leg_r_link10" friction="0.01" />
		<xacro:default_link_collision link_name="leg_r_link11" friction="0.0" />
		<xacro:default_link_collision link_name="leg_r_link12_1" friction="0.0" />
		<xacro:default_link_collision link_name="leg_r_link12_2" friction="0.5" />
	</xacro:macro>
</robot>